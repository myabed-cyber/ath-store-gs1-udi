# === Required (Northflank Secrets) ===
DATABASE_URL=postgresql://USER:PASSWORD@HOST:6543/postgres?sslmode=require
JWT_SECRET_EFFECTIVE=CHANGE_ME_LONG_RANDOM
SEED_ADMIN_USER=admin
SEED_ADMIN_PASS=<set-strong-password>
SEED_OPERATOR_USER=operator
SEED_OPERATOR_PASS=<set-strong-password>

# === Optional ===
NODE_ENV=production
PORT=8080
AUTO_SCHEMA=true
# Comma-separated. Example: https://your-app.domain,https://custom.domain
CORS_ORIGINS=

SEED_MODE=upsert
SEED_STRICT_BOOTSTRAP=true

# If your DATABASE_URL password contains special characters like @, URL-encode them (e.g., @ => %40).
